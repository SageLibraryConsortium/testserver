    /*Firefox - Sage*/
}
button,
input {
    font-family: 'Open Sans', Arial, Helvetica, sans-serif;
    font-size: 14px;
}

select {
    font-family:inherit;
    font-weight:normal;
}

select.radius {
    border-radius: 3px; }
select:disabled {
    background-color: #DDDDDD;
    cursor: default; }
    

*/

.sr-only {
    position: absolute;
    width: 1px;
    height: 1px;
    padding: 0;
    margin: -1px;
    overflow: hidden;
    clip: rect(0, 0, 0, 0);
    border: 0;
}
/* Make added rows in Expert Search have bold labels like the initial row */

label[for*=expert_] {
    font-weight: bold;
}
}
.sort_deemphasize {
    font-weight: lighter;
    font-size: 70%;
}
